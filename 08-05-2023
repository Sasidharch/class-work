max and min in array
#include <stdio.h>
void main() {
  int a[100],i,n,min,max;
  printf("Enter the size of the array :");
  scanf("%d",&n);
  printf("Enter the elements :");
  for(i = 0;i <= n;i++){
    scanf("%d",&a[i]);
  }
  min = max = a[0];
  for(i = 1;i <= n;i++) {
    if(min>a[i])
      min = a[i];
    if(max<a[i])
      max = a[i];
  }
  printf("The minimum value is : %d",min);
  printf("The maximum value is : %d",max);
}
2.merge in array
#include<stdio.h>
void main(){
  int a[100],i,n,m,b[100];
  printf("Enter the size of a :");
  scanf("%d",&n);
  printf("Enter the elements :");
  for(i = 0;i <= n;i++){
    scanf("%d",a[i]);
  }
  printf("Enter the size of b :");
  scanf("%d",&m);
  printf("Enter the elements :");
  for(i = 0;i <= m;i++){
    scanf("%d",&b[i]);
  }
  int c[n + m];
  for(i = 0;i < n;i++){
    c[i] = a[i];
  }
  for(i = 0;i < m;i++){
    c[n + i] = b[i];
  }
  for(i = 0;i < m+n;i++){
    printf("%d",c[i]);
  }
  }
  3.min in array
  #include<stdio.h>
void main(){
  int a[100],i,n,m,b[100];
  printf("Enter the size of a :");
  scanf("%d",&n);
  printf("Enter the elements :");
  for(i = 0;i <= n;i++){
    scanf("%d",a[i]);
  }
  printf("Enter the size of b :");
  scanf("%d",&m);
  printf("Enter the elements :");
  for(i = 0;i <= m;i++){
    scanf("%d",&b[i]);
  }
  int c[n + m];
  for(i = 0;i < n;i++){
    c[i] = a[i];
  }
  for(i = 0;i < m;i++){
    c[n + i] = b[i];
  }
  for(i = 0;i < m+n;i++){
    printf("%d",c[i]);
  }
  }
  4.deltion of duplicates
  #include <stdio.h>  
#include <conio.h>  
int main ()  
{  
    int arr[20], i, j, k, size;        
    printf (" Define the number of elements in an array: ");  
    scanf (" %d", &size);        
    printf (" \n Enter %d elements of an array: \n ", size);  
    for ( i = 0; i < size; i++)  
    {  
        scanf (" %d", &arr[i]);  
    }  
    for ( i = 0; i < size; i ++)  
    {  
        for ( j = i + 1; j < size; j++)  
        {  
            if ( arr[i] == arr[j])  
            {  
                for ( k = j; k < size - 1; k++)  
                {  
                    arr[k] = arr [k + 1];  
                }  
                size--;  

                j--;      
            }  
        }  
    }        
    printf (" \n Array elements after deletion of the duplicate elements: ");  
          for ( i = 0; i < size; i++)  
    {  
        printf (" %d \t", arr[i]);  
    }  
    return 0;  
}  
5.insert element using array
#include <stdio.h>
void main()
{
    int array[100];
    int i, n, x, pos;
 
    printf("Enter the number of elements in the array \n");
    scanf("%d", &n);
    printf("Enter the elements \n");
 
    for (i = 0; i < n; i++)
    {
        scanf("%d", &array[i]);
    }
 
    printf("Input array elements are: \n");
    for (i = 0; i < n; i++)
    {
        printf("%d ", array[i]);
    }
    printf("\nEnter the new element to be inserted: ");
    scanf("%d", &x);
    printf("Enter the position where element is to be inserted: ");
    scanf("%d", &pos);
 
    
    n=n+1;
    for(i = n-1; i >= pos; i--)
        array[i]=array[i-1];
 
    array[pos-1]=x; 
    for (i = 0; i < n; i++)
    {
        printf("%d ", array[i]);
    }
}
